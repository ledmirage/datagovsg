{
    "name": "ledmirage/datagovsg",
    "description": "Interface to data.gov.sg API as a Laravel 5 package",
    "keywords": [
        "ledmirage",
        "datagovsg"
    ],
    "homepage": "https://github.com/ledmirage/datagovsg",
    "license": "MIT",
    "authors": [
        {
            "name": "xieer",
            "email": "xieer86@gmail.com",
            "homepage": "https://twitter.com/xieer86",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "guzzlehttp/guzzle": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Ledmirage\\Datagovsg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ledmirage\\Datagovsg\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
